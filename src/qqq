.PS
scale=2.54
cct_init
log_init
include(lib_base.ckt)
include(lib_color.ckt)
include(lib_stm32.ckt)

#RR:register_shadow(ARR);
#line -> from RR.s down_ 1;
#register(count);
#internal_port_in(R)
#gpio_port(1,L, C)
#prescaler(16)

CC: controller(, 4,3); {"TIM2" at CC.ne rjust above;}
RR: register_shadow(ARR) at CC.n + (0, -0.5);
line -> from RR.s down_ 0.5;
CN: register(count);
line -> from CN.s down_ 0.5;
register_shadow(ARR)
.PE
